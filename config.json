{
  "src": {
    "js": "./app/**/*.js",
    "css": "./app/**/*.css",
    "html": "./app/**/!(index)*.html",
    "index": "./app/index.html"
  },
  "dist": {
    "path": "build/",
    "js": {
      "app": "app.js",
      "vendor": "vendor.js"
    },
    "css": {
      "app": "app.css",
      "vendor": "vendor.css"
    }
  }
}